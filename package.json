{
  "name": "dontcode",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "align-version": "PACKAGE_VERSION=$(npm view @dontcode/plugin-seed versions --json | jq -r '.[-1]') && cd libs/seed && npm version $PACKAGE_VERSION",
    "push-all": "cd libs/seed && npm version prerelease && cd ../.. && nx run seed:build:development && cd dist/libs/seed && yalc push",
    "push": "cd libs/${npm_config_dir} && npm version prerelease && cd ../.. && nx run ${npm_config_dir}:build:development && cd dist/libs/${npm_config_dir} && yalc push"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation-runtime": "~15.0.3",
    "@angular/animations": "^15.1.2",
    "@angular/cdk": "^15.1.2",
    "@angular/common": "^15.1.2",
    "@angular/compiler": "^15.1.2",
    "@angular/core": "^15.1.2",
    "@angular/forms": "^15.1.2",
    "@angular/platform-browser": "^15.1.2",
    "@angular/platform-browser-dynamic": "^15.1.2",
    "@angular/router": "^15.1.2",
    "@dontcode/core": "^0.6.2",
    "@dontcode/plugin-common": "^0.6.2",
    "@dontcode/sandbox": "^0.6.2",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primeng": "^15.1.1",
    "fake-indexeddb": "^4.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.4.1",
    "zone.js": "^0.12.0"
  },
  "devDependencies": {
    "@angular-architects/module-federation": "~15.0.3",
    "@angular-devkit/build-angular": "^15.1.3",
    "@angular-eslint/eslint-plugin": "^15.2.0",
    "@angular-eslint/eslint-plugin-template": "^15.2.0",
    "@angular-eslint/template-parser": "^15.2.0",
    "@angular/cli": "~15.1.0",
    "@angular/compiler-cli": "^15.1.2",
    "@angular/language-service": "^15.1.2",
    "@nrwl/angular": "15.6.3",
    "@nrwl/cli": "15.6.3",
    "@nrwl/cypress": "15.6.3",
